<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_signin_and_sign_up_mobile_prototype2"
    tools:context=".MainActivity2">

    <TextView
        android:id="@+id/textView20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/mont_bold"
        android:freezesText="false"
        android:gravity="center"
        android:singleLine="false"
        android:text="Sign Up"
        android:textAlignment="center"
        android:textColor="@color/signin"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/mont_regular"
        android:gravity="center"
        android:text="Please complete your\n biodata correctly"
        android:textAlignment="center"
        android:textColor="#C1BAAA"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView20" />


    <ScrollView
        android:id="@+id/scrollView20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/nameedittexte"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="@dimen/_12sdp"
                android:layout_marginEnd="32dp"
                android:textColorHint="#C0C0C2">

                <EditText
                    android:id="@+id/nameedittext"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:background="@drawable/backshapewe"
                    android:drawablePadding="16dp"
                    android:ems="10"
                    android:fontFamily="@font/mont_regular"
                    android:hint="Name"

                    android:inputType="textPersonName"
                    android:padding="@dimen/_6sdp"
                    android:paddingStart="@dimen/_24sdp"
                    android:textColor="#000"
                    android:textColorHint="#C0C0C2" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/emaildittexte"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="@dimen/_24sdp"
                android:textColorHint="#C0C0C2">

                <EditText
                    android:id="@+id/email"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:background="@drawable/backshapewe"
                    android:drawablePadding="@dimen/_12sdp"
                    android:ems="10"
                    android:fontFamily="@font/mont_regular"

                    android:hint="Email"
                    android:inputType="textPersonName"
                    android:padding="@dimen/_6sdp"
                    android:paddingStart="@dimen/_12sdp"
                    android:textColor="#000"
                    android:textColorHint="#C0C0C2" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/passwordedittexte"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="32dp"
                android:textColorHint="#C0C0C2"
                app:endIconDrawable="@drawable/togglepassword"
                app:endIconMode="password_toggle">

                <EditText
                    android:id="@+id/password"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:background="@drawable/backshapewe"
                    android:drawablePadding="@dimen/_12sdp"
                    android:ems="10"
                    android:fontFamily="@font/mont_regular"

                    android:hint="Password"
                    android:inputType="textPassword"
                    android:padding="@dimen/_6sdp"
                    android:paddingStart="@dimen/_12sdp"
                    android:textColor="#000"
                    android:textColorHint="#C0C0C2" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/repasswordedittexte"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"

                android:layout_marginTop="32dp"
                android:layout_marginEnd="32dp"
                android:textColorHint="#C0C0C2"
                app:endIconDrawable="@drawable/togglepassword"
                app:endIconMode="password_toggle">

                <EditText
                    android:id="@+id/repassword"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"

                    android:background="@drawable/backshapewe"
                    android:drawablePadding="@dimen/_12sdp"
                    android:ems="10"
                    android:fontFamily="@font/mont_regular"
                    android:hint="RE Password"

                    android:inputType="textPassword"
                    android:padding="@dimen/_6sdp"
                    android:paddingStart="@dimen/_12sdp"
                    android:textColor="#000"
                    android:textColorHint="#C0C0C2" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/adressedittexte"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="32dp"

                android:layout_marginEnd="32dp"
                android:textColorHint="#C0C0C2">

                <EditText
                    android:id="@+id/adressedittext"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"

                    android:background="@drawable/backshapewe"
                    android:drawablePadding="16dp"
                    android:ems="10"
                    android:fontFamily="@font/mont_regular"
                    android:hint="Adresss"

                    android:inputType="textPersonName"
                    android:padding="@dimen/_6sdp"
                    android:paddingStart="@dimen/_12sdp"
                    android:textColor="#000"
                    android:textColorHint="#C0C0C2" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/phoneedittexte3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="32dp"
                android:textColorHint="#C0C0C2">

                <EditText
                    android:id="@+id/phoneedittext3"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"

                    android:background="@drawable/backshapewe"
                    android:drawablePadding="@dimen/_12sdp"
                    android:ems="10"
                    android:fontFamily="@font/mont_regular"
                    android:hint="Phone Number"

                    android:inputType="phone"
                    android:padding="@dimen/_6sdp"
                    android:paddingStart="@dimen/_12sdp"
                    android:textColor="#000"
                    android:textColorHint="#C0C0C2" />
            </com.google.android.material.textfield.TextInputLayout>

            <Button
                android:id="@+id/signups"
                style="@style/TextAppearance.AppCompat.Widget.Button"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/_24sdp"
                android:layout_marginTop="@dimen/_24sdp"
                android:layout_marginRight="@dimen/_24sdp"
                android:background="@drawable/shapeback"
                android:fontFamily="@font/mont_bold"
                android:text="Sign up"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView18" />

            <TextView
                android:id="@+id/btnsignin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="@dimen/_15sdp"
                android:fontFamily="@font/mont_regular"
                android:gravity="center"
                android:text="Already have an account Sign in"
                android:textAlignment="center"
                android:textColor="@color/signin"
                android:textStyle="bold" />

        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>